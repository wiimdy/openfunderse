# NextAuth
AUTH_SECRET=
ADMIN_LOGIN_ID=admin
# Prefer hash in shared env; plain password is allowed for local testing.
ADMIN_LOGIN_PASSWORD=change_me
# ADMIN_LOGIN_PASSWORD_HASH=$2a$10$...

# Claw relayer runtime
NODE_ENV=development
RELAYER_PORT=3100
# Supabase Data API (required)
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_DEFAULT_KEY=
RPC_URL=http://127.0.0.1:8545
CHAIN_ID=10143
CLAIM_FINALIZATION_MODE=OFFCHAIN

# Claim attestation EIP-712 domain verifier address (required).
CLAIM_ATTESTATION_VERIFIER_ADDRESS=

# Contracts (set after deploy)
# Required only when CLAIM_FINALIZATION_MODE=ONCHAIN
CLAIM_BOOK_ADDRESS=
INTENT_BOOK_ADDRESS=
CLAW_VAULT_ADDRESS=
CLAW_FUND_FACTORY_ADDRESS=

# URLs
RELAYER_URL=http://127.0.0.1:3100
INDEXER_URL=http://127.0.0.1:3200
EVIDENCE_STORE_URL=http://127.0.0.1:9000

# Signing / submitter
RELAYER_SIGNER_PRIVATE_KEY=

# TODO: replace with exact policy once verifier registry shape is fixed.
VERIFIER_ALLOWLIST=

# Admin RBAC (comma separated ids)
ADMIN_IDS=admin

# Bot auth
# Format: BOT_API_KEYS="botA:keyA,botB:keyB"
BOT_API_KEYS=bot-strategy-1:replace_me,bot-crawler-1:replace_me,bot-verifier-1:replace_me
# Format: BOT_SCOPES="botA:scope1|scope2,botB:scope1"
BOT_SCOPES=bot-strategy-1:intents.propose|bots.register,bot-crawler-1:claims.submit,bot-verifier-1:claims.attest|intents.attest

# Aggregator weighted thresholds
CLAIM_THRESHOLD_WEIGHT=3
INTENT_THRESHOLD_WEIGHT=5
VERIFIER_WEIGHT_SNAPSHOT=0x00000000000000000000000000000000000000a1:2,0x00000000000000000000000000000000000000b2:2,0x00000000000000000000000000000000000000c3:1
MAX_SUBMIT_RETRIES=3

# Execution cron
CLAW_CORE_ADDRESS=
EXECUTOR_PRIVATE_KEY=
EXECUTION_BATCH_LIMIT=5
EXECUTION_MAX_ATTEMPTS=5
CRON_SECRET=

# Optional: viem struct payload example (`npm run -w @claw/relayer factory:create-fund`)
# FUND_OWNER=
# STRATEGY_AGENT=0x0000000000000000000000000000000000000000
# SNAPSHOT_BOOK=
# ASSET=
# VAULT_NAME=Fund Vault Share
# VAULT_SYMBOL=FVS
# NADFUN_LENS=0x0000000000000000000000000000000000000000
# INITIAL_VERIFIERS=
# INITIAL_VERIFIER_WEIGHTS=
# INITIAL_ALLOWED_TOKENS=
# INITIAL_ALLOWED_ADAPTERS=
