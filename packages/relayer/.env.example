# NextAuth
AUTH_SECRET=
ADMIN_LOGIN_ID=admin
# Prefer hash in shared env; plain password is allowed for local testing.
ADMIN_LOGIN_PASSWORD=change_me
# ADMIN_LOGIN_PASSWORD_HASH=$2a$10$...

# Claw relayer runtime
# Do not set NODE_ENV in Vercel project env vars.
# `next build` must run with NODE_ENV=production.
RELAYER_PORT=3100
# Supabase Data API (required)
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_DEFAULT_KEY=
RPC_URL=https://testnet-rpc.monad.xyz
CHAIN_ID=10143
CLAIM_FINALIZATION_MODE=OFFCHAIN

# Claim attestation EIP-712 domain verifier address (required).
CLAIM_ATTESTATION_VERIFIER_ADDRESS=0x76AB8571cED9F4A81b733969216F53C1e13BC835

# Contracts (set after deploy)
# Required only when CLAIM_FINALIZATION_MODE=ONCHAIN
CLAIM_BOOK_ADDRESS=
INTENT_BOOK_ADDRESS=0x76AB8571cED9F4A81b733969216F53C1e13BC835
CLAW_VAULT_ADDRESS=0x34FB76f115f4C4E461eB90eA9f92faEF8c4A5E0A
CLAW_FUND_FACTORY_ADDRESS=0xf16a12fCeC5FD8eb67eEd57f9659AE730734AA74

# URLs
RELAYER_URL=http://127.0.0.1:3100
INDEXER_URL=http://127.0.0.1:3200
EVIDENCE_STORE_URL=http://127.0.0.1:9000

# TODO: replace with exact policy once verifier registry shape is fixed.
VERIFIER_ALLOWLIST=

# Admin RBAC (comma separated ids)
ADMIN_IDS=admin

# Bot auth
# Format: BOT_API_KEYS="botA:keyA,botB:keyB"
BOT_API_KEYS=bot-strategy-1:replace_me,bot-participant-1:replace_me
# Format: BOT_SCOPES="botA:scope1|scope2,botB:scope1"
BOT_SCOPES=bot-strategy-1:intents.propose|bots.register|funds.bootstrap,bot-participant-1:claims.submit|claims.attest|intents.attest

# Aggregator weighted thresholds
CLAIM_THRESHOLD_WEIGHT=3
INTENT_THRESHOLD_WEIGHT=5
VERIFIER_WEIGHT_SNAPSHOT=0x00000000000000000000000000000000000000a1:2,0x00000000000000000000000000000000000000b2:2,0x00000000000000000000000000000000000000c3:1
MAX_SUBMIT_RETRIES=3

# Execution cron (disabled in keyless mode; strategy signer bot executes onchain)
CLAW_CORE_ADDRESS=0xE8240A3A0eF6953Bf51D28Be0635E4e94CaE2522
EXECUTOR_PRIVATE_KEY=
EXECUTION_BATCH_LIMIT=5
EXECUTION_MAX_ATTEMPTS=5
CRON_SECRET=

# Fund creation/deployment via admin routes is disabled.
# Strategy can still sync already-deployed fund metadata via:
# POST /api/v1/funds/sync-by-strategy (requires CLAW_FUND_FACTORY_ADDRESS)
