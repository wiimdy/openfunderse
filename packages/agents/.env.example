# Legacy combined template (strategy + participant).
# Prefer role-specific templates:
# - .env.strategy.example
# - .env.participant.example
#
# ==================================================
# COMMON (all roles)
# ==================================================
# Relayer endpoint + chain/rpc
RELAYER_URL=http://127.0.0.1:3100
CHAIN_ID=10143
RPC_URL=https://testnet-rpc.monad.xyz

# Default bot credentials (used by strategy, and participant fallback)
BOT_ID=bot-strategy-1
BOT_API_KEY=replace_me
BOT_ADDRESS=0x22dda95Deb3B959EC2C9d82fD9d7FA0765f4aeBe

# Shared onchain contract
INTENT_BOOK_ADDRESS=0x76AB8571cED9F4A81b733969216F53C1e13BC835

# ==================================================
# STRATEGY BOT ONLY
# ==================================================
# Strategy submission safety
STRATEGY_AUTO_SUBMIT=false
STRATEGY_REQUIRE_EXPLICIT_SUBMIT=true
STRATEGY_TRUSTED_RELAYER_HOSTS=127.0.0.1,localhost
STRATEGY_ALLOW_HTTP_RELAYER=true

# Strategy runtime contracts
CLAW_FUND_FACTORY_ADDRESS=0xf16a12fCeC5FD8eb67eEd57f9659AE730734AA74
VAULT_ADDRESS=0x34FB76f115f4C4E461eB90eA9f92faEF8c4A5E0A
CLAW_CORE_ADDRESS=0xE8240A3A0eF6953Bf51D28Be0635E4e94CaE2522

# NadFun config (Monad testnet)
NADFUN_LENS_ADDRESS=0xB056d79CA5257589692699a46623F901a3BB76f1
NADFUN_BONDING_CURVE_ROUTER=0x865054F0F6A288adaAc30261731361EA7E908003
NADFUN_DEX_ROUTER=0x5D4a4f430cA3B1b2dB86B9cFE48a5316800F5fb2
NADFUN_WMON_ADDRESS=0xFb8bE43D65FBC1290D6178C6DbA6E58c6D18fA60
NADFUN_EXECUTION_ADAPTER_ADDRESS=0x7949DBd0779Cd829D1B2491A33C8EcCc88669Aa4
# Alias accepted by strategy runtime (same as NADFUN_EXECUTION_ADAPTER_ADDRESS)
ADAPTER_ADDRESS=0x7949DBd0779Cd829D1B2491A33C8EcCc88669Aa4

# Strategy policy defaults
STRATEGY_MAX_IMPACT_BPS=60
STRATEGY_SELL_TAKE_PROFIT_BPS=2000
STRATEGY_SELL_STOP_LOSS_BPS=600
STRATEGY_SELL_MAX_HOLD_SECONDS=5400
STRATEGY_DEADLINE_MIN_SECONDS=600
STRATEGY_DEADLINE_BASE_SECONDS=900
STRATEGY_DEADLINE_MAX_SECONDS=3600
STRATEGY_DEADLINE_PER_CLAIM_SECONDS=20

# Strategy signer (EOA)
STRATEGY_PRIVATE_KEY=0xYOUR_STRATEGY_PRIVATE_KEY
STRATEGY_CREATE_MIN_SIGNER_BALANCE_WEI=10000000000000000

# ==================================================
# PARTICIPANT BOT ONLY
# ==================================================
# Participant-scoped bot credentials
PARTICIPANT_BOT_ID=bot-participant-1
PARTICIPANT_BOT_API_KEY=replace_me
PARTICIPANT_BOT_ADDRESS=0xf40C22fce49835867AC6599d03e9c391a1CFF912

# Participant signing key (recommended)
PARTICIPANT_PRIVATE_KEY=0xYOUR_PARTICIPANT_PRIVATE_KEY

# Participant submission safety
PARTICIPANT_AUTO_SUBMIT=false
PARTICIPANT_REQUIRE_EXPLICIT_SUBMIT=true
PARTICIPANT_TRUSTED_RELAYER_HOSTS=127.0.0.1,localhost
PARTICIPANT_ALLOW_HTTP_RELAYER=true

# Participant claim defaults (optional)
# PARTICIPANT_DEFAULT_HORIZON_SEC=3600

# ==================================================
# OPTIONAL / LEGACY COMPAT
# ==================================================
# If PARTICIPANT_PRIVATE_KEY is missing, signer falls back in this order:
# BOT_PRIVATE_KEY -> VERIFIER_PRIVATE_KEY
# BOT_PRIVATE_KEY=0xYOUR_BOT_PRIVATE_KEY
# VERIFIER_PRIVATE_KEY=0xYOUR_VERIFIER_PRIVATE_KEY

# # Legacy role addresses (optional)
# CRAWLER_ADDRESS=0x1111111111111111111111111111111111111111
# VERIFIER_ADDRESS=0x2222222222222222222222222222222222222222
