# ==================================================
# PARTICIPANT BOT TEMPLATE
# ==================================================
# Copy this file for participant bot runtime only.
#
# Example:
# cp packages/agents/.env.participant.example /tmp/participant.env
# set -a; source /tmp/participant.env; set +a

# Common runtime
RELAYER_URL=https://openfunderse-relayer-liard.vercel.app/
CHAIN_ID=10143
RPC_URL=https://testnet-rpc.monad.xyz

# Shared onchain contracts
INTENT_BOOK_ADDRESS=0x76AB8571cED9F4A81b733969216F53C1e13BC835
CLAIM_ATTESTATION_VERIFIER_ADDRESS=0x76AB8571cED9F4A81b733969216F53C1e13BC835

# Participant-scoped bot identity (preferred)
PARTICIPANT_BOT_ID=bot-participant-1
PARTICIPANT_BOT_API_KEY=replace_me
PARTICIPANT_BOT_ADDRESS=0xf40C22fce49835867AC6599d03e9c391a1CFF912

# Participant signer
PARTICIPANT_PRIVATE_KEY=0xYOUR_PARTICIPANT_PRIVATE_KEY

# Participant submission safety
PARTICIPANT_AUTO_SUBMIT=false
PARTICIPANT_REQUIRE_EXPLICIT_SUBMIT=true
PARTICIPANT_TRUSTED_RELAYER_HOSTS=127.0.0.1,localhost
PARTICIPANT_ALLOW_HTTP_RELAYER=true

# Participant source / crawl safety
PARTICIPANT_ALLOWED_SOURCE_HOSTS=www.reddit.com,api.coingecko.com
PARTICIPANT_MAX_RESPONSE_BYTES=524288
PARTICIPANT_ALLOW_HTTP_SOURCE=false
REDDIT_USER_AGENT=openclaw-mvp-crawler/0.1 (+https://github.com/wiimdy/agent)

# Optional fallback only (prefer PARTICIPANT_PRIVATE_KEY)
# BOT_PRIVATE_KEY=0xYOUR_BOT_PRIVATE_KEY
# VERIFIER_PRIVATE_KEY=0xYOUR_VERIFIER_PRIVATE_KEY

# Legacy fallback only (prefer CLAIM_ATTESTATION_VERIFIER_ADDRESS)
CLAIM_BOOK_ADDRESS=0x0000000000000000000000000000000000000000


# # Fallback bot identity (used when PARTICIPANT_BOT_* is not set)
# BOT_ID=bot-participant-1
# BOT_API_KEY=replace_me
# BOT_ADDRESS=0xf40C22fce49835867AC6599d03e9c391a1CFF912
