{
  "name": "@wiimdy/openfunderse-agents",
  "version": "0.1.6",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prebuild": "npm --prefix ../sdk run build",
    "build": "tsc -p tsconfig.json",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "participant:allocation": "tsx src/index.ts participant-allocation",
    "participant:join": "tsx src/index.ts participant-join",
    "participant:daemon": "tsx src/index.ts participant-daemon",
    "strategy:create:fund": "tsx src/index.ts strategy-create-fund",
    "strategy:attest:onchain": "tsx src/index.ts strategy-attest-onchain",
    "strategy:execute:ready": "tsx src/index.ts strategy-execute-ready",
    "strategy:propose": "tsx src/index.ts strategy-propose",
    "strategy:dry-run:intent": "tsx src/index.ts strategy-dry-run-intent",
    "clawbot:run": "tsx src/index.ts clawbot-run",
    "bot:smoke:e2e": "node scripts/clawbot-e2e-smoke.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@claw/protocol-sdk": "npm:@wiimdy/openfunderse-sdk@^0.1.0",
    "eventsource": "^3.0.7",
    "viem": "^2.38.2"
  },
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.18",
    "vitest": "^4.0.18"
  }
}
